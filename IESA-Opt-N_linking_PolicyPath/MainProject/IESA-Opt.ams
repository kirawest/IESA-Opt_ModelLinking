					{tf | flex_range(tf)='1 week [w]'}
				}
			}
			Set tech_flexM {
				SubsetOf: tech_flexible;
				Index: tf_m;
				Definition: {
					{tf | flex_range(tf)='1 month [m]'}
				}
			}
			Set tech_flexS {
				SubsetOf: tech_flexible;
				Index: tf_s;
				Definition: {
					{tf | flex_range(tf)='1 season [s]'}
				}
			}
			Set tech_flexB {
				SubsetOf: tech_flexible;
				Index: tf_b;
