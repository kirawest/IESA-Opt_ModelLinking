				IndexDomain: (nn,d,ad,p);
				InitialData: 0;
			}
			Parameter refProfiles_D {
				IndexDomain: (nn,d,tg,p);
				InitialData: 0;
			}
			Parameter finProfiles_D {
				IndexDomain: (nn,d,tg,p);
				InitialData: 0;
			}
